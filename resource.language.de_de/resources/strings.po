msgid "D65 (standard)"
msgstr "D65 (Standard)"

msgctxt "#36587"
msgid "D93 (Japanese NTSC)"
msgstr "D93 (NTSC Japan)"

msgctxt "#36588"
msgid "Automatic"
msgstr "Automatisch"

msgctxt "#36589"
msgid "HDTV (BT.709)"
msgstr "HDTV (BT.709)"

msgctxt "#36590"
msgid "SDTV (SMPTE C)"
msgstr "SDTV (SMPTE C)"

