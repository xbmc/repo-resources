msgid "Disabled"
msgstr "Onemogućeno"

#. name of a stereoscopic mode
msgctxt "#36503"
msgid "Over / Under"
msgstr "Iznad/Ispod"

#. name of a stereoscopic mode
msgctxt "#36504"
msgid "Side by side"
msgstr "Usporedno"

#. name of a stereoscopic mode
msgctxt "#36505"
msgid "Anaglyph red / cyan"
msgstr "Stereoskopska slika crveno/svjetloplavo"

#. name of a stereoscopic mode
