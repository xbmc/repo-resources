msgstr "Távirányítógomb lenyomás utáni felengedési idő (ms)"

msgctxt "#36050"
msgid "On start"
msgstr "Indításkor"

msgctxt "#36099"
msgid "Dithering"
msgstr "Ditherelés"

msgctxt "#36100"
