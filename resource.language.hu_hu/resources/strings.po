msgstr "* Elem mappa"

msgctxt "#36042"
msgid "Use limited colour range (16-235)"
msgstr "Korlátozott színtér használata (16-235)"

msgctxt "#36043"
msgid "Number of buffers used by graphics driver"
msgstr "A grafikus eszközmeghajtó által használt pufferek száma"

msgctxt "#36044"
