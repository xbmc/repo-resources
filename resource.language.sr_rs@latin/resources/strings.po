msgctxt "#36508"
msgid "Hardware based"
msgstr "Zasnovano na hardveru"

#. name of a stereoscopic mode
#: system/settings/settings.xml
msgctxt "#36509"
msgid "Monoscopic / 2D"
msgstr "Monoskopski / 2D"

#. name of a stereoscopic mode
#: system/settings/settings.xml
msgctxt "#36510"
msgid "Anaglyph yellow / blue"
msgstr "Anaglif žuto / plavo"

#. name of a stereoscopic mode
#: system/settings/settings.xml
