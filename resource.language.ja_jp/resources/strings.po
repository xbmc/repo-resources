msgid "D65 (standard)"
msgstr "D65 (standard)"

msgctxt "#36587"
msgid "D93 (Japanese NTSC)"
msgstr "D93 (Japanese NTSC)"

#. primaries labels
msgctxt "#36588"
msgid "Automatic"
msgstr "自動"

msgctxt "#36589"
msgid "HDTV (BT.709)"
msgstr "HDTV (BT.709)"

msgctxt "#36590"
msgid "SDTV (SMPTE C)"
msgstr "SDTV (SMPTE C)"

